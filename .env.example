# Environment variables for Docker Compose
# Copy this to .env and update with your DagHub values

# DagHub credentials (required for MLflow tracking)
DAGSHUB_USERNAME=your_dagshub_username
DAGSHUB_TOKEN=your_dagshub_token

# MLflow settings
ENABLE_MLFLOW=true

# Model settings
BATCH_SIZE=16
EPOCHS=20
LEARNING_RATE=0.0002